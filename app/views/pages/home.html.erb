<% if user_signed_in? %>
  <div>
    <%= @current_user.username %>
  </div>
  <div>
    <%= @current_user.email %>
  </div>
  <div>
    <%= @current_user.profile %>
  </div>
<% else %>
  <h1>ようこそ</h1>
  <p>トップページです。</p>
  <% if Rails.env.development? %>開発環境です<% end %>
  <% if Rails.env.production? %>本番環境です<% end %>
<% end %>
